{
  "expo": {
    "name": "food-app",
    "slug": "food-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/deliveroo-loading.jpg",
      "resizeMode": "contain"
    },
    "ios": {
      "bundleIdentifier": "com.ishan.foodapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ishan.foodapp",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#00CCBC"
      },
      "splash": {
        "image": "./assets/images/deliveroo-loading.jpg",
        "resizeMode": "contain"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-localization",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
